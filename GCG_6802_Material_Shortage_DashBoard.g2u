	f.Intrinsic.Control.EndIf
	
	gui.F_Shortage.GsgcResult.AddGridviewFromDatatable("gvOrders", "dtOrders")
	gui.F_Shortage.GsgcResult.SetGridviewProperty("gvOrders", v.Enum.GridViewPropertyNames!EnableAppearanceEvenRow, True)
	gui.F_Shortage.GsgcResult.SetGridviewProperty("gvOrders", v.Enum.GridViewPropertyNames!Editable, False)
	gui.F_Shortage.GsgcResult.SetGridviewProperty("gvOrders", v.Enum.GridViewPropertyNames!ReadOnly, True)
	gui.F_Shortage.GsgcResult.SetGridviewProperty("gvOrders", v.Enum.GridViewPropertyNames!SuppressNothingDates, True)
