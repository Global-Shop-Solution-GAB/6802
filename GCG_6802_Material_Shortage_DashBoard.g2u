'Get Data 
'6802
Program.Sub.GetData.Start
f.Intrinsic.Control.Try
	v.Local.sSQL.Declare(String)
	f.Intrinsic.String.Build("select RTRIM(WORK_ORDER) WORKORDER, RTRIM(WORK_ORDER_SUFFIX) SUFFIX, RTRIM(CUSTOMER_NAME) CUSTOMER, RTRIM(WO_PART) WO_PART, RTRIM(WO_DESC) WO_PARTDESC, WO_DUEDATE, RTRIM(PART) PART, RTRIM(DESCRIPTION) PART_DESC, UM, QTY_REQ, QTY_ONHAND, PO_NUMBER, PO_DUE, PO_QTY_ORD, PO_QTY_RCVD, PO_VENDOR from GCG_6802_MAT_SHORT where TERMINAL = '{0}' order by WORKORDER, SUFFIX, PART, PO_QTY_ORD, PO_NUMBER", v.Caller.Terminal, v.Local.sSQL)
	
	'f.ODBC.Connection!con.OpenCompanyConnection
		f.Intrinsic.Control.If(v.DataTable.dtOrders.Exists)
			f.Data.Datatable.Close("dtOrders")
		f.Intrinsic.Control.EndIf
		f.Data.Datatable.CreateFromSQL("dtOrders", "Con", v.Local.sSQL, True)
	
	'Overall Demand by Part Dict
	f.Data.Dictionary.CreateFromSQL("dictOverallDemand", "Con", "select RTRIM(PART), SUM(HOURS_ESTIMATED) from V_JOB_OPERATIONS  where PART <> '' and LMO = 'M' and FLAG_CLOSED <> 'Y' and RTRIM(JOB)+'-'+RTRIM(SUFFIX) not in (select RTRIM(REF.JOB)+'-'+RTRIM(REF.SUFFIX) from V_JOB_HEADER REF where DATE_CLOSED>'1900-01-01') group by PART")
	f.Data.Dictionary.SetDefaultReturn("dictOverallDemand", 0)
	
	f.Data.Dictionary.CreateFromSQL("dictSODemand", "Con", "select RTRIM(PART), SUM(QTY_ORDERED-QTY_SHIPPED) from V_ORDER_LINES group by PART")
	f.Data.Dictionary.SetDefaultReturn("dictSODemand",0)
	
	f.Intrinsic.Control.If(v.DataTable.dtOrders.RowCount,>,0)
		f.Data.Datatable.AddColumn("dtOrders", "OVERALL_DEMAND_WO", Float)
		f.Data.Datatable.AddColumn("dtOrders", "OVERALL_DEMAND_SO", Float)
		f.Data.Datatable.FillFromDictionary("dtOrders", "dictOverallDemand", "PART", "OVERALL_DEMAND_WO")
		f.Data.Datatable.FillFromDictionary("dtOrders", "dictSODemand", "PART", "OVERALL_DEMAND_SO")
		f.Data.Datatable.AddExpressionColumn("dtOrders", "OVERALL_DEMAND",Float, "OVERALL_DEMAND_WO + OVERALL_DEMAND_SO")
	f.Intrinsic.Control.EndIf
	
	f.Data.Dictionary.Close("dictOverallDemand")
	f.Data.Dictionary.Close("dictSODemand")
	'f.ODBC.Connection!con.Close
	
f.Intrinsic.Control.Catch
	f.Intrinsic.Control.CallSub(Catching)
f.Intrinsic.Control.EndTry
Program.Sub.GetData.End

'Format
