'Export to Excel
Program.Sub.ExportToExcelFile.Start
f.Intrinsic.Control.Try
	v.Local.sFileName.Declare(String)
	v.Local.boolExcelExists.Declare(Boolean)
	f.Automation.MSExcel.CheckPresence(v.Local.boolExcelExists)
	
	f.Intrinsic.String.Build("Material Shortage Report {0}", v.Ambient.Now.FormatMM-DD-YYYY, v.Local.sFileName)
	
	'file dialog
	f.Intrinsic.UI.ShowSaveFileDialog(v.Local.sFileName, "*.xls", "", v.Local.sFileName)

	'CANCEL
	f.Intrinsic.Control.If(v.Local.sFileName,=,"***CANCEL***")
		'unload
		f.Intrinsic.Control.ExitSub
	f.Intrinsic.Control.EndIf
	
	'check if Excel exist
	f.Intrinsic.Control.If(v.Local.boolExcelExists)
		'export to excel
		f.Intrinsic.String.Build("{0}{1}",v.Local.sFileName,".xlsx",v.Local.sFileName)
		gui.F_Shortage.GsgcResult.Export(v.Local.sFileName,"xlsx")
	
	'no Excel
	f.Intrinsic.Control.Else
		'export to csv
		f.Intrinsic.String.Build("{0}{1}",v.Local.sFileName,".csv",v.Local.sFileName)
		gui.F_Shortage.GsgcResult.Export(v.Local.sFileName,"csv")
	
	f.Intrinsic.Control.EndIf
	
	'execute
	F.Intrinsic.Task.ShellExec(0,"",V.Local.sFileName,"","",1)
f.Intrinsic.Control.Catch
	f.Intrinsic.Control.CallSub(Catching)
f.Intrinsic.Control.EndTry
Program.Sub.ExportToExcelFile.End

