	gui.F_Shortage.GsgcResult.SetGridviewProperty("gvOrders", "OptionsViewShowAutoFilterRow", true)
	
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "WO_PART", v.Enum.ColumnPropertyNames!Caption, "WO PART")
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "WO_PARTDESC", v.Enum.ColumnPropertyNames!Caption, "WO PART DESCRIPTION")
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "PART_DESC", v.Enum.ColumnPropertyNames!Caption, "PART DESCRIPTION")
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "QTY_REQ", v.Enum.ColumnPropertyNames!Caption, "QTY REQ'D")
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "QTY_ONHAND", v.Enum.ColumnPropertyNames!Caption, "QTY ONHAND")
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "PO_NUMBER", v.Enum.ColumnPropertyNames!Caption, "PO NUMBER")
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "PO_DUE", v.Enum.ColumnPropertyNames!Caption, "PO DUE DATE")
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "PO_QTY_ORD", v.Enum.ColumnPropertyNames!Caption, "PO QTY")
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "PO_VENDOR", v.Enum.ColumnPropertyNames!Caption, "PO VENDOR")
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "WO_DUEDATE", v.Enum.ColumnPropertyNames!Caption, "WO DUE DATE")
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "OVERALL_DEMAND", v.Enum.ColumnPropertyNames!Caption, "OVERALL DEMAND")
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "OVERALL_DEMAND_WO", v.Enum.ColumnPropertyNames!Caption, "OVERALL DEMAND WO")
	gui.F_Shortage.GsgcResult.SetColumnProperty("gvOrders", "OVERALL_DEMAND_SO", v.Enum.ColumnPropertyNames!Caption, "OVERALL DEMAND SO")
